<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IS_DATEOFSALE_YTD__c</fullName>
    <description>Indicates if the sales date can be used in YTD calculations</description>
    <externalId>false</externalId>
    <formula>(DateOfSale__c - DATE(YEAR(DateOfSale__c), 01, 01))
&lt;=
(TODAY() - DATE(YEAR( TODAY()), 01,01))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IsDateOfSaleYTD</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
